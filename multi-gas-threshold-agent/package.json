{
  "name": "high-gas-used-bot",
  "version": "0.1.1",
  "agentId": "nethforta-1",
  "description": "Bot that detects transactions with unusually high gas used",
  "chainIds": [
    1
  ],
  "repository": "https://github.com/NethermindEth/Forta-Agents/tree/main/multi-gas-threshold-agent",
  "scripts": {
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,ts,json  --exec 'npm run build && forta-agent run'",
    "start:prod": "forta-agent run --prod",
    "tx": "forta-agent run --tx",
    "block": "forta-agent run --block",
    "range": "forta-agent run --range",
    "file": "forta-agent run --file",
    "publish": "forta-agent publish",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "forta-agent": "^0.1.16",
    "forta-agent-tools": "^3.2.3",
    "node-fetch": "^2.6.8"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/nodemon": "^1.19.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.8",
    "prettier": "^2.8.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  }
}
